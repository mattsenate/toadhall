function highlight_links(show) {
    var links_cell = document.getElementById('editable-links');
    links_cell.style.backgroundColor = show ? '#fff' : 'inherit';
    var edit_links_div = document.getElementById('edit-links-link');
    edit_links_div.style.visibility = show ? 'visible' : 'hidden';
}
</script>
